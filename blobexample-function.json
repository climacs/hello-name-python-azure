{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "inputBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "myblobcontainer/input/{name}",
      "connection": "MyStorageConnectionAppSetting"
    },
    {
      "name": "outputBlob",
      "type": "blob",
